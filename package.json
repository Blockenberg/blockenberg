{
  "name": "blockenberg",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build && fission-cli-ubuntu-20.04-x86_64 app publish && surge build --domain blockenberg.surge.sh",
    "test": "ava src/**/*.test.ts",
    "test:e2e": "playwright test",
    "report:e2e": "playwright show-report",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@playwright/test": "^1.31.0",
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.8.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/gtag.js": "^0.0.12",
    "@types/qrcode-svg": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.13",
    "ava": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "events": "^3.3.0",
    "one-webcrypto": "^1.0.1",
    "postcss": "^8.4.21",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-preprocess": "^5.0.1",
    "svelte-seo": "^1.5.2",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "type": "module",
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "nodeArguments": [
      "--loader=./tsnode-loader.js"
    ]
  },
  "dependencies": {
    "@tiptap/core": "^2.0.0-beta.212",
    "@tiptap/extension-image": "^2.0.0-beta.212",
    "@tiptap/extension-link": "^2.0.0-beta.218",
    "@tiptap/extension-placeholder": "^2.0.0-beta.212",
    "@tiptap/pm": "^2.0.0-beta.212",
    "@tiptap/starter-kit": "^2.0.0-beta.212",
    "clipboard-copy": "^4.0.1",
    "multiformats": "^11.0.1",
    "qrcode-svg": "^1.1.0",
    "sanitize-html": "^2.10.0",
    "uint8arrays": "^4.0.2",
    "webnative": "^0.35.1"
  },
  "engines": {
    "node": ">=16.14"
  }
}
